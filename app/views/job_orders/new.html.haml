.div{:style => "position: relative; top: 75px; z-index:1; left: 275px; width: 80%;"}
  -if(flash[:errors]!=nil)
    - flash[:errors].each do |msg|
      .alert.alert-danger{:style => " left: 8px;"}= msg
  -flash.clear
  = form_for(:job_order, :url => '/job_orders') do |job|
    = render(:partial => "request_form", :locals => {:job => job})
    .float-right{:style => "padding-bottom: 25px; padding-right: 15px;"}
      %button.btn.btn-primary{:type => "submit"}
        %i.fa.fa-check>
          Submit
      = link_to(job_order_tracking_system_path, method: :get, class: 'btn btn-secondary') do
        %i.fa.fa-times>
          Cancel
        

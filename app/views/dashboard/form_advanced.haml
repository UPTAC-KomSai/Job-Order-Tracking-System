  / form input mask
  .col-md-6.col-sm-12.col-xs-12
    .x_panel
      .x_title
        %h2 Input Mask
        %ul.nav.navbar-right.panel_toolbox
          %li
            %a.collapse-link
              %i.fa.fa-chevron-up
          %li.dropdown
            %a.dropdown-toggle{"aria-expanded" => "false", "data-toggle" => "dropdown", :href => "#", :role => "button"}
              %i.fa.fa-wrench
            %ul.dropdown-menu{:role => "menu"}
              %li
                %a{:href => "#"} Settings 1
              %li
                %a{:href => "#"} Settings 2
          %li
            %a.close-link
              %i.fa.fa-close
        .clearfix
      .x_content
        %br/
        %form.form-horizontal.form-label-left
          .form-group
            %label.control-label.col-md-3.col-sm-3.col-xs-3 Date Mask
            .col-md-9.col-sm-9.col-xs-9
              %input.form-control{"data-inputmask" => "'mask': '99/99/9999'", :type => "text"}/
              %span.fa.fa-user.form-control-feedback.right{"aria-hidden" => "true"}
          .form-group
            %label.control-label.col-md-3.col-sm-3.col-xs-3 Phone mask
            .col-md-9.col-sm-9.col-xs-9
              %input.form-control{"data-inputmask" => "'mask' : '(999) 999-9999'", :type => "text"}/
              %span.fa.fa-user.form-control-feedback.right{"aria-hidden" => "true"}
          .form-group
            %label.control-label.col-md-3.col-sm-3.col-xs-3 Custom Mask
            .col-md-9.col-sm-9.col-xs-9
              %input.form-control{"data-inputmask" => "'mask': '99-999999'", :type => "text"}/
              %span.fa.fa-user.form-control-feedback.right{"aria-hidden" => "true"}
          .form-group
            %label.control-label.col-md-3.col-sm-3.col-xs-3 Serial Number
            .col-md-9.col-sm-9.col-xs-9
              %input.form-control{"data-inputmask" => "'mask' : '****-****-****-****-****-***'", :type => "text"}/
              %span.fa.fa-user.form-control-feedback.right{"aria-hidden" => "true"}
